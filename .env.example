# OpenAI compatible endpoint configuration
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5-mini
DEV_MODEL=gpt-5
TECHLEAD_MODEL=gpt-5-mini
WORKSPACE_PATH=/workspace
GIT_REMOTE_URL=
GIT_AUTHOR_NAME=Orchestrator Agent
GIT_AUTHOR_EMAIL=orchestrator@example.local

# Optional GitHub auth for creating branches, PRs, and comments
GITHUB_TOKEN=

# Repo configuration
